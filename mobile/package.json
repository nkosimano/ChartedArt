    "@react-navigation/native-stack": "^6.11.0",
    "@supabase/supabase-js": "^2.47.10",
    "expo": "~54.0.0",
    "expo-constants": "~17.0.3",
    "expo-device": "~7.0.1",
    "expo-file-system": "~18.0.4",
